import java.util.Scanner;
class Sample{
 
    
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int sum=sc.nextInt();
        
        
        do {
 
           
            sum += x;
            x--;
        }
 
        
        while (x > 10);
 
        
        System.out.println("The sum is: " + sum);
    }
}
